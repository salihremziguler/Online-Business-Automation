﻿@using MvcOnlineTicariOtomasyon.Models.Classes
@model List<CargoTracking>
@{
    ViewBag.Title = "DetailCargo";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<br />
<table class="table table-striped">
    <tr>
        <th>Açıklama</th>
        <th>Tarih</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Explanation</td>
            <td>@x.date.ToString("dd/MM/yyyy")</td>
        </tr>
    }
</table>
<a class="btn btn-secondary" href="/Cargo/Index" style="width:100px;color:white;position:absolute;right:25px"><i class="fas fa-backspace"></i>  İptal</a>

